{
	"name": "Domain Time Tracker",
	"description": "This extension track how much time you cost in every domain",
	"icons": {"128": "icon.jpg", "48": "icon48.jpg"},
	"version": "1.0",
	"permissions": ["tabs", "https://*/*", "http://*/*"],
	"background_page": "background.html",
	"browser_action": {
		"default_title": "Browse Time Tracker",
		"default_icon": "icon.jpg",
		"popup": "popup.html"
	},
	"content_scripts": [{
		"matches": ["<all_urls>"],
		"js": ["js/contentscript.js"]
	}]
}
